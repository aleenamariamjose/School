<apex:page controller="WizardDemoController">
        <apex:form >
            <apex:pageBlock title="New Contact Wizard: Step 2">
                <apex:pageBlockButtons location="top">
                    <apex:commandButton action="{!goToPageThree}" value="Next"/>
                    <apex:commandButton action="{!goToPageOne}" value="Previous"/>
                </apex:pageBlockButtons>

                <apex:pageBlockSection >
                        <apex:outputText value="Email" />
                        <apex:inputText value="{!email}" />
                </apex:pageBlockSection>
            </apex:pageBlock>
        </apex:form>
</apex:page>
